{
    "name": "comicrelief/campaign",
    "description": "Campaign profile using Drupal 8.",
    "type": "project",
    "license": "GPL 2.0+",
    "authors": [
        {
            "name": "Comic Relief",
            "email": "info@comicrelief.com",
            "homepage": "http://comicrelief.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@comicrelief.com",
        "source": "https://github.com/comicrelief/campaign"
    },
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "8.1.*",
        "drupal/console": "~1.0",
        "drush/drush": "~8.0",
        "drupal/admin_toolbar": "1.x-dev#32b7111",
        "drupal/cdn": "3.0-beta1",
        "drupal/config_devel": "1.x-dev",
        "drupal/config_readonly": "1.0-beta2",
        "drupal/crazyegg": "1.*",
        "drupal/ctools": "3.0-alpha26",
        "drupal/default_content": "1.x-dev#c6a79ee",
        "drupal/devel": "1.x-dev#b8bcdc6",
        "drupal/diff": "1.0-alpha2",
        "drupal/ds": "2.4",
        "drupal/editor_advanced_link": "1.2",
        "drupal/embed": "1.0-rc2",
        "drupal/entity": "1.0-alpha3",
        "drupal/entity_browser": "1.x-dev#77e18608cff42cfc4384bbcdd730a1ffdc37779b",
        "drupal/entity_embed": "1.x-dev#08e5098909f5286238edfb716353efa651eb5a5d",
        "drupal/entity_reference_revisions": "1.*",
        "drupal/fast_404": "1.*",
        "drupal/field_group": "1.*",
        "drupal/file_entity": "2.0-beta1",
        "drupal/focal_point": "1.x-dev#d39b83f955281255811618403ab62ee572146f0f",
        "drupal/imagemagick": "1.*",
        "drupal/inline_entity_form": "1.0-alpha6",
        "drupal/layout_plugin": "1.0-alpha22",
        "drupal/media_entity": "1.0",
        "drupal/media_entity_browser": "1.x-dev#1f9a47a",
        "drupal/media_entity_instagram": "1.0",
        "drupal/media_entity_image": "1.0",
        "drupal/media_entity_slideshow": "1.0",
        "drupal/media_entity_twitter": "1.0",
        "drupal/memcache": "2.x-dev#908c0a2",
        "drupal/menu_link_attributes": "1.*",
        "drupal/metatag": "1.0-beta9",
        "drupal/optimizely": "2.15-rc1",
        "drupal/page_manager": "1.0-alpha23",
        "drupal/panelizer": "3.0-alpha2",
        "drupal/panels": "3.x-dev#a5400d7",
        "drupal/paragraphs": "1.*",
        "drupal/pathauto": "1.0-alpha3",
        "drupal/purge": "3.0-beta4",
        "drupal/purge_purger_http": "1.0-beta3",
        "drupal/rabbitmq": "1.*",
        "drupal/redirect": "1.*",
        "drupal/scheduled_updates": "1.*",
        "drupal/simple_sitemap": "2.5",
        "drupal/social_links": "1.x-dev",
        "drupal/stage_file_proxy": "1.*",
        "drupal/token": "1.0-alpha2",
        "drupal/video_embed_field": "1.1",
        "drupal/view_mode": "1.*",
        "drupal/view_modes_display": "1.x-dev",
        "drupal/cookieconsent": "^1.1",
        "drupal/search_api": "1.x-dev#895b507",
        "drupal/youtube": "1.0-beta3",
        "drupal/yamlform": "1.0-beta20",
        "drupal/yamlform_queue": "1.x-dev",
        "mikeyp/google_tag": "dev-8.x-1.x",
        "phing/phing": "^2.15"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "package",
            "package": {
                "name": "bestiejs/platform.js",
                "version": "v1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://raw.githubusercontent.com/bestiejs/platform.js/0870795f0c277a33cc2078a96e74434f89aa11d8/platform.js",
                    "type": "zip"
                }
            }
        },
        "2": {
            "type": "package",
            "package": {
                "name": "cbopp-art/lightcase",
                "version": "v2.3.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/cbopp-art/lightcase/archive/v2.3.4.zip",
                    "type": "zip"
                }
            }
        },
        "3": {
            "type": "package",
            "package": {
                "name": "previousnext/drush_cmi_tools",
                "version": "v1",
                "type": "drupal-drush",
                "dist": {
                    "url": "https://raw.githubusercontent.com/previousnext/drush_cmi_tools/ac3a1b71acf843a52807c7362357d8acb7a366c0/drush_cmi_tools.drush.inc",
                    "type": "zip"
                }
            }
        },
        "4": {
            "type": "vcs",
            "url": "https://github.com/mikeyp/google_tag"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "profiles/cr/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/cr/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "profiles/{$name}": [
                "type:drupal-profile"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Redirect to install.php on empty DB. Required for platform.sh": "https://www.drupal.org/files/issues/728702-install-redirect_7.patch",
                "List unmet dependencies instead of just failing": "https://www.drupal.org/files/issues/missing-dependencies-2655104-8.patch",
                "Call to a member function getPath() when installing Profiles with unknown modules as requirement": "https://www.drupal.org/files/issues/core-profile_unknown_module-2608408-45.patch",
                "Retain destination url after preview": "https://www.drupal.org/files/issues/2325463-42-node-preview-workflow-fixed.patch",
                "Constraint violation for paragraph editing, for full reasoning see https://github.com/comicrelief/campaign/pull/252": "https://gist.githubusercontent.com/pvhee/14801e0f82e06f51cb0d88b9d400cbd6/raw/253f1c1e516eb24650a91d343c56a5aeb6a0ca43/PLAT-235_paragraph_constraint_violation.patch",
                "See https://www.drupal.org/node/2314185": "https://www.drupal.org/files/issues/drupalbehaviorerror-2314185-15.patch"
            },
            "drupal/config_devel": {
                "Show differences between config & info.yml": "https://www.drupal.org/files/issues/show_differences_between-2801201-2.patch"
            },
            "drupal/ctools": {
                "EntityFieldDeriver naive about field info, throws PHP warnings": "https://www.drupal.org/files/issues/entityfieldderiver-2672110-8.patch"
            },
            "drupal/media_entity": {
                "hook_requirements-have-to-return-an-array": "https://www.drupal.org/files/issues/2717375-hook_requirements-have-to-return-an-array.patch"
            },
            "mikeyp/google_tag": {
                "Patch from https://gist.github.com/pvhee/166ddfb556b9d1e2870b903e1b54847d. Apply this patch if not Travis builds in RND17 etc. will fail on Behat tests. See e.g. https://travis-ci.com/comicrelief/rnd17/builds/30910019": "https://gist.githubusercontent.com/pvhee/166ddfb556b9d1e2870b903e1b54847d/raw/94e2936f57e725c3afa0f450ec32a24f9e5698d6/google_tag_broken_tests.patch"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*",
        "drupal/link-link": "*",
        "drupal/crop": "*"
    }
}
