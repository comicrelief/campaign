{
    "name": "comicrelief/campaign",
    "type": "drupal-profile",
    "description": "Basic profile using Drupal 8.",
    "authors": [
        {
            "name": "Comic Relief",
            "email": "info@comicrelief.com",
            "homepage": "http://comicrelief.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@comicrelief.com",
        "source": "https://github.com/comicrelief/campaign"
    },
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.0",
        "drupal/console": "~1.0",
        "drush/drush": "~8.0",
        "drupal/admin_toolbar": "1.14",
        "drupal/cdn": "3.*",
        "drupal/config_devel": "1.*",
        "drupal/config_readonly": "1.*",
        "drupal/crazyegg": "1.*",
        "drupal/ctools": "3.0-alpha27",
        "drupal/default_content": "1.*",
        "drupal/devel": "1.*",
        "drupal/diff": "1.0-alpha2",
        "drupal/ds": "2.*",
        "drupal/editor_advanced_link": "1.*",
        "drupal/embed": "1.*",
        "drupal/entity": "1.0-alpha3",
        "drupal/entity_browser": "1.*",
        "drupal/entity_embed": "1.*",
        "drupal/entity_reference_revisions": "1.*",
        "drupal/fast_404": "1.*",
        "drupal/field_group": "1.*",
        "drupal/file_entity": "2.0-beta1",
        "drupal/focal_point": "1.*",
        "drupal/imagemagick": "1.*",
        "drupal/inline_entity_form": "1.0-alpha6",
        "drupal/layout_plugin": "1.0-alpha22",
        "drupal/media_entity": "1.*",
        "drupal/media_entity_browser": "1.*",
        "drupal/media_entity_instagram": "1.*",
        "drupal/media_entity_image": "1.*",
        "drupal/media_entity_slideshow": "1.*",
        "drupal/media_entity_twitter": "1.*",
        "drupal/memcache": "2.*",
        "drupal/menu_link_attributes": "1.*",
        "drupal/metatag": "1.*",
        "drupal/optimizely": "2.*",
        "drupal/page_manager": "1.0-alpha23",
        "drupal/panelizer": "3.0-alpha2",
        "drupal/panels": "3.0-beta5",
        "drupal/paragraphs": "1.*",
        "drupal/pathauto": "1.0-alpha3",
        "drupal/purge": "3.*",
        "drupal/purge_purger_http": "1.*",
        "drupal/rabbitmq": "1.*",
        "drupal/redirect": "1.*",
        "drupal/scheduled_updates": "1.*",
        "drupal/simple_sitemap": "2.*",
        "drupal/social_links": "1.*",
        "drupal/stage_file_proxy": "1.*",
        "drupal/token": "1.0-alpha2",
        "drupal/video_embed_field": "1.*",
        "drupal/view_mode": "1.*",
        "drupal/view_modes_display": "1.*",
        "phing/phing": "^2.15"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "bestiejs/platform.js",
                "version": "v1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://raw.githubusercontent.com/bestiejs/platform.js/0870795f0c277a33cc2078a96e74434f89aa11d8/platform.js",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "cbopp-art/lightcase",
                "version": "v2.3.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/cbopp-art/lightcase/archive/v2.3.4.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "previousnext/drush_cmi_tools",
                "version": "v1",
                "type": "drupal-drush",
                "dist": {
                    "url": "https://raw.githubusercontent.com/previousnext/drush_cmi_tools/ac3a1b71acf843a52807c7362357d8acb7a366c0/drush_cmi_tools.drush.inc",
                    "type": "zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Redirect to install.php on empty DB": "https://www.drupal.org/files/issues/728702-install-redirect_7.patch",
                "List unmet dependencies instead of just failing": "https://www.drupal.org/files/issues/missing-dependencies-2655104-8.patch",
                "Call to a member function getPath() when installing Profiles with unknown modules as requirement": "https://www.drupal.org/files/issues/core-profile_unknown_module-2608408-45.patch"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
