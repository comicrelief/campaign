personal_information:
  '#title': 'Your Personal Information'
  '#type': details
  '#open': true
  first_name:
    '#title': 'First Name'
    '#type': textfield
    '#required': true
  last_name:
    '#title': 'Last Name'
    '#type': textfield
    '#required': true
contact_information:
  '#title': 'Your Contact Information'
  '#type': details
  '#open': true
  email:
    '#title': Email
    '#type': email
    '#required': true
  phone:
    '#title': Phone
    '#type': tel
    '#required': true
  address:
    '#title': Address/Apt
    '#type': textfield
    '#required': true
  city:
    '#title': City
    '#type': textfield
    '#required': true
  state_province:
    '#title': 'State/Province'
    '#type': select
    '#options': state_province_names
    '#required': true
  zip_code:
    '#title': 'Zip / Country Code'
    '#type': textfield
    '#required': true
  country:
    '#title': 'Country'
    '#type': select
    '#options': country_names
    '#required': true
