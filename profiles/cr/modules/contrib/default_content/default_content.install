<?php

/**
 * @file
 * Installs default content on the behalf of modules at time of install.
 */


/**
 * Implements hook_install().
 * check all existing modules for imported content()
 */
function default_content_install() {
  foreach (\Drupal::moduleHandler()->getModuleList() as $module => $null) {
    \Drupal::service('default_content.manager')->importContent($module);
  }
}
